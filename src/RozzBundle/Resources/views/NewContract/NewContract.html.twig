{% extends 'base.html.twig' %}

{% block body %}



<div class="new-contract-container">
    <section class="new-contract-navigation">
        <button id="contract-types-btn" class="btn btn-success">Вид договор</button> <br>
        <button id="selected-lands-btn" class="btn">Имоти</button> <br>
        <button id="holders-btn" class="btn">Наемател</button> <br>
        <button id="contract-data-btn" class="btn">Срок/Заповед</button> <br>
        <button id="preview-btn" class="btn">Преглед/Запазване</button> <br>
        <button class="btn btn-danger">Отказ</button> <br>
    </section>


    <section class="new-contract">
        <form action="">
            <div class="contract-types active">
                <label for="contract-types">Вид Договор :</label>
                <select class="form-control" name="types" id="contract-types">
                    <option value="1">Едногодишен договор</option>
                    <option value="2">Многогодишен договор</option>
                    <option value="3">Анекс към договор</option>
                </select>
            </div>

            <div class="selected-lands">
                <div class="jumbotron" style="padding: 30px">
                    <legend>
                        Определи площа на избраните имоти за новия договор
                    </legend>
                    <span style="color: red">Няма избрани имоти</span>
                </div>
            </div>
            <div class="holder">
                <h1>Holgers</h1>
            </div>
            <div class="contract-data">
                <h1>Contract Data</h1>
            </div>
            <div class="contract-preview">



